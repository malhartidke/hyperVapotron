FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      epsilon.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -3 0 0 0 0 ];

internalField   uniform 1e-4;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    fuWall
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    uWall
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    buWall
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    chamberWall
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    fluid_to_solid
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
}


