FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/fluid";
    object      U.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform (4 0 0);

boundaryField
{
#includeEtc "caseDicts/setConstraintTypes"
    inlet
    {
        type            fixedValue;
        value           uniform (4 0 0);
    }
    outlet
    {
        type            pressureInletOutletVelocity;
        phi             phi.liquid;
        value           uniform (4 0 0);
    }
    fuWall
    {
        type            noSlip;
    }
    uWall
    {
        type            noSlip;
    }
    buWall
    {
        type            noSlip;
    }
    chamberWall
    {
        type            noSlip;
    }
    fluid_to_solid
    {
        type            noSlip;
    }
}


